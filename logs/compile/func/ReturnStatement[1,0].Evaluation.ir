def <SYNTAX>:ReturnStatement[1,0].Evaluation(
  this: Ast:ReturnStatement[1,0],
): Unknown {
  sdo-call %0 = this[0]->Evaluation()
  let exprRef = %0
  call %1 = clo<GetValue>(exprRef)
  let exprValue = [? %1]
  call %2 = clo<GetGeneratorKind>()
  if (= %2 ~async~) {
    call %3 = clo<Await>(exprValue)
    exprValue = [? %3]
  } else {}
  return comp[~return~/~empty~](exprValue)
}